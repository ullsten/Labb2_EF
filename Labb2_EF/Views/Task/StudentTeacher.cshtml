@model IEnumerable<Labb2_EF.Models.JoinModels.StudentsTeachers>

@{
    ViewData["Title"] = "StudentTeacher";
}

<h1>StudentTeacher</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Students)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Teachers)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Students.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Teachers.TeacherFirstName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StudentsTeachersId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StudentsTeachersId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StudentsTeachersId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
